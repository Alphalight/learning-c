#include <stdio.h>



int main()
{

float v1,v2,v3,m;
int p1=1,p2=2,p3=3;

printf("Inserir a primeira nota\n");
scanf("%f", &v1);
printf("Inserir a secunda nota\n");
scanf("%f", &v2);
printf("Inserir a terceira nota\n");
scanf("%f", &v3);

m = (p1*v1 + p2*v2 + p3*v3)/(p1+p2+p3);

printf("A media ponderada e:%.2f\n",m);


return 0;

}

